  border-top-right-radius: 30rpx;
  display: inline-block;
  animation:noticemove2 1s infinite;
  animation-iteration-count:1;
}


@keyframes noticemove
{
	from {top:100vh;}
	to {top:34vh;}
}
@keyframes noticemove2
{
