  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  display: inline-block;
}
.noticemove{
  position: fixed;
  z-index: 999999;
  left:0;
  top: 30vh;
  width: 100%;
  height:70vh;
  background: #ffffff;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  display: inline-block;
  animation:noticemove 1s infinite;
  animation-iteration-count:1;
}
.noticemove2{
  position: fixed;
  z-index: 999999;
  left:0;
  top: 100vh;
  width: 100%;
  height:70vh;
